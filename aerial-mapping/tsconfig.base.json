{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@aerial-mapping/apps/api": ["apps/api/src/app/index.ts"],
      "@aerial-mapping/api/dashboard": ["libs/api/dashboard/feature/src/index.ts"],
      "@aerial-mapping/api/dashboard/service/feature": [
        "libs/api/dashboard/service/feature/src/index.ts"
      ],
      "@aerial-mapping/api/shared/services/prisma/data-access": [
        "libs/api/shared/services/prisma/data-access/src/index.ts"
      ],
      "@aerial-mapping/api/shell/feature": [
        "libs/api/shell/feature/src/index.ts"
      ],
      "@aerial-mapping/apps/api": ["apps/api/src/app/index.ts"],
      "@aerial-mapping/client/account": ["libs/client/account/src/index.ts"],
      "@aerial-mapping/client/dashboard": [
        "libs/client/dashboard/src/index.ts"
      ],
      "@aerial-mapping/client/file-upload": [
        "libs/client/file-upload/src/index.ts"
      ],
      "@aerial-mapping/client/image-catalogue": [
        "libs/client/image-catalogue/src/index.ts"
      ],
      "@aerial-mapping/client/login": ["libs/client/login/src/index.ts"],
      "@aerial-mapping/client/mapcollection": [
        "libs/client/mapcollection/src/index.ts"
      ],
      "@aerial-mapping/client/register": ["libs/client/register/src/index.ts"],
      "@aerial-mapping/client/shared/assets": [
        "libs/client/shared/assets/src/index.ts"
      ],
      "@aerial-mapping/client/shared/components/footer": [
        "libs/client/shared/components/footer/src/index.ts"
      ],
      "@aerial-mapping/client/shared/components/navbar": [
        "libs/client/shared/components/navbar/src/index.ts"
      ],
      "@aerial-mapping/data": ["libs/data/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
